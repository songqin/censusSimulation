set xrange [0:1]
set yrange [0:1]
set terminal pdf
set output 'out.pdf'
set key horizontal bottom left 
set xlabel "false positive rate"
set ylabel "true positive rate"

set title "ROC curve identifies eligible agents for attacker:type1, nonattacker:type2"

set label "ap=10%, nap=90%, nawp=0\%" at 0.70,0.47 center
set label "awfp, area under the curve (AUC)" at 0.5,0.2 center
plot 'attackerWitness0.00' using 2:1 with linespoints title "0%, 0.514",\
'attackerWitness0.01' using 2:1 with linespoints title "1%, 0.474",\
'attackerWitness0.02' using 2:1 with linespoints title "2%, 0.442",\
x title ""
#pause -1
#quit











